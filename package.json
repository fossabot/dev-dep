  },
  "bugs": {
    "url": "https://github.com/dr-js/dev-dependency-pack/issues"
  },
  "scripts": {
    "example": "cross-env NODE_ENV=production babel source --out-dir example",
    "example-dev": "cross-env NODE_ENV=development babel source --out-dir example --watch",
    "library": "cross-env NODE_ENV=production babel source --out-dir library",
    "library-dev": "cross-env NODE_ENV=development babel source --out-dir library --watch",
    "prepare": "npm run build",
    "build": "npm run library",
    "start": "npm run example-dev"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
