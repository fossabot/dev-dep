{
  "private": true,
  "name": "dev-dep-tool",
  "version": "0.0.14-dev.8",
  "author": "dr-js",
  "license": "MIT",
  "description": "Provide common package devDependencies",
  "keywords": [
    "Dr",
    "Dr-js",
    "JavaScript"
  ],
  "repository": "https://github.com/dr-js/dev-dep.git",
  "homepage": "https://github.com/dr-js/dev-dep#readme",
  "bugs": "https://github.com/dr-js/dev-dep/issues",
  "bin": "bin/index.js",
  "sideEffects": false,
  "scripts": {
    "// script ======================": "",
    "script-base": "cross-env BABEL_ENV=dev babel-node ./script",
    "script-pack": "cross-env BABEL_ENV=dev babel-node ./script pack",
    "script-publish": "cross-env BABEL_ENV=dev babel-node ./script pack publish",
    "script-publish-dev": "cross-env BABEL_ENV=dev babel-node ./script pack publish-dev",
    "script-pack-package": "cross-env BABEL_ENV=dev babel-node ./script pack pack-package",
    "script-pack-package-publish": "cross-env BABEL_ENV=dev babel-node ./script pack pack-package publish",
    "script-pack-package-publish-dev": "cross-env BABEL_ENV=dev babel-node ./script pack pack-package publish-dev",
    "script-generate-export": "cross-env BABEL_ENV=dev babel-node ./script/generateExport",
    "// dev =========================": "",
    "build-library-dev": "cross-env NODE_ENV=development BABEL_ENV=dev babel ./source --out-dir ./output-gitignore/library --watch",
    "build-bin-dev": "cross-env NODE_ENV=development BABEL_ENV=dev babel ./source-bin --out-dir ./output-gitignore/bin --watch",
    "// build =======================": "",
    "build-library": "cross-env NODE_ENV=production BABEL_ENV=library babel ./source --out-dir ./output-gitignore/library",
    "build-bin": "cross-env NODE_ENV=production BABEL_ENV=library babel ./source-bin --out-dir ./output-gitignore/bin",
    "// minify =======================": "",
    "minify-library": "cross-env BABEL_ENV=dev babel-node ./script/uglify library",
    "// =============================": "",
    "check-outdated": "node ./output-gitignore/bin -C -i ./resource",
    "check-outdated-self": "node ./output-gitignore/bin -C -i ./package.json",
    "check-outdated-node-module": "node ./output-gitignore/bin -C -i ./node_modules/dev-dep-babel/package.json",
    "prepack": "echo \"Error: should not prepack this package\" && exit 1"
  },
  "engines": {
    "node": ">=8.8.0",
    "npm": ">=5.5.0"
  },
  "dependencies": {
    "dr-js": "^0.12.0"
  },
  "devDependencies": {
    "dev-dep-babel": "^0.0.14-dev.8"
  }
}
