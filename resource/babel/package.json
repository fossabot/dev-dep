{
  "INSTALL": [ ".babelrc" ],
  "scripts": {
    "build-library": "cross-env NODE_ENV=production BABEL_ENV=library babel ./source --out-dir ./library",
    "build-library-dev": "cross-env NODE_ENV=development BABEL_ENV=library-dev babel ./source --out-dir ./library --watch"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0"
  }
}
